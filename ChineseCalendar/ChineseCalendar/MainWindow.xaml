<Window x:Class="ChineseCalendar.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ChineseCalendar"
        mc:Ignorable="d"
        Title="MainWindow" Height="261" Width="488">
    <Grid>
        <Label x:Name="TitleLabel" Content="Chinese Calendar" HorizontalAlignment="Center" Margin="0,27,0,0" VerticalAlignment="Top"/>
        <Label x:Name="EventLabel" Content="Today's Event" HorizontalAlignment="Left" Margin="44,73,0,0" VerticalAlignment="Top"/>
        <Button x:Name="CalendarButton" Content="View Calendar" HorizontalAlignment="Center" Margin="0,172,0,0" VerticalAlignment="Top" Height="33" Width="130" Click="CalendarButton_Click"/>

    </Grid>
</Window>
